Algoritmo Suma
	Definir Num1, Num2, Resultado Como Entero;
	Num1 <- 0;
	Num2 <- 0;
	Resultado <- 0;
	Escribir "Este es un algoritmo para sumar 2 números. Por favor, escriba el 1er número.";
	Leer Num1;
	Escribir "Escriba el 2do número.";
	Leer Num2;
	Resultado <- Num1 + Num2;
	Escribir "El resultado de la suma entre ", Num1, " y ", Num2, " es de ", Resultado;
FinAlgoritmo
